source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :ios, '9.0'

def share_VendorTools
  pod 'SDWebImage', '~> 4.0'
end

target 'Benyue' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  share_VendorTools

  target 'BenyueTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BenyueUITests' do
    # Pods for testing
  end

end
